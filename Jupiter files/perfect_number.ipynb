   "source": [
    "Number = int(input(\"Pls enter a number here \"))\n",
    "\n",
    "Value = 1\n",
    "Sum = 0\n",
    "\n",
    "while Value < Number:\n",
    "    if Number % Value == 0:\n",
    "        Sum += Value\n",
    "    Value += 1\n",
    "    \n",
    "if Sum == Number:\n",
    "    print(\"Your number is a perfect number\")\n",
    "    \n",
    "else:\n",
    "    print(\"It's not a perfect number \")"
   ]
